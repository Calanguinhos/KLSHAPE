        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView17"
        android:layout_alignRight="@+id/imageView17"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:text="JoÃ£o da SIlva"
        android:textColor="@android:color/background_light"
        android:layout_below="@+id/textViewNomeAtleta" />

    <TextView
        android:id="@+id/textViewNomeAtleta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView8"
        android:layout_alignLeft="@+id/imageView8"
        android:layout_alignStart="@+id/imageView8"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:text="@android:string/defaultMsisdnAlphaTag"
        android:textColor="@android:color/background_light" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/info_treinadorr"
        android:layout_below="@+id/imageView11"
        android:layout_alignRight="@+id/textViewNomeTreinador"
        android:layout_alignEnd="@+id/textViewNomeTreinador" />

    <TextView
        android:id="@+id/textViewNomeTreinador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView8"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_toEndOf="@+id/imageView13"
        android:layout_toRightOf="@+id/imageView13"
